<style>
    .product_count {
        display: inline-block;
        position: relative;
        margin-bottom: 24px;
    }

    .product_count label {
        font-size: 14px;
        color: #777777;
        font-family: "Roboto", sans-serif;
        font-weight: normal;
        padding-right: 10px;
    }

    .product_count input {
        width: 76px;
        border: 1px solid #eeeeee;
        border-radius: 3px;
        padding-left: 10px;
    }

    .product_count button {
        display: block;
        border: none;
        background: transparent;
        box-shadow: none;
        cursor: pointer;
        position: absolute;
        right: 0px;
        font-size: 14px;
        color: #cccccc;
        -webkit-transition: all 0.3s ease 0s;
        -moz-transition: all 0.3s ease 0s;
        -o-transition: all 0.3s ease 0s;
        transition: all 0.3s ease 0s;
    }

    .product_count button:hover {
        color: #222222;
    }

    .product_count .increase {
        top: -4px;
    }

    .product_count .reduced {
        bottom: -8px;
    }
</style>
<!-- Shop Cart Section Begin -->
<!-- Breadcrumb Begin -->
<div class="breadcrumb-option">
    <div class="container">

        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb__links">
                    <a href="/">
                        <i class="fa fa-home"></i>
                        Home</a>
                    <span>Cart</span>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcrumb End -->


{{#if cartData.products}}
<section class="shop-cart spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-6">
                {{#each cartData.products}}
                <div class="card mb-3" style="max-width: 540px;">
                    <div class="row no-gutters">
                        <div class="col-md-4 d-flex align-items-center">
                            <img src="/product-images/{{this.product.img.[0]}}" class="card-img">
                        </div>
                        <div class="col-md-8">
                            <div class="card-body">
                                <a href="/productDetails/{{this._id}}"
                                    class="card-title font-weight-bold text-dark">{{this.product.productName}}</a>
                                <h6 class="card-title ">{{this.product.brandName}}</h6>
                                {{!-- <p class="card-text">This is a wider card with supporting text below as a natural
                                    lead-in
                                    to
                                    additional content. This content is a little bit longer.</p> --}}
                                <div class="">
                                    <div class="d-flex">
                                        <p class="card-text text-danger font-weight-bold pr-2 mb-1">₹
                                            {{this.product.price}}
                                        </p>
                                        {{!-- <p class="card-text"><small class="text-muted"><del>₹ 1700</del></small>
                                        </p> --}}
                                    </div>
                                </div>
                                <p class="card-text"><small class="text-muted">Delivery by one week.</small></p>
                                {{!-- <div class="cart__quantity">
                                    <div class="pro-qty">
                                        <input type="text" value={{this.quantity}}>
                                    </div>
                                </div> --}}

                                <div class="product_count" id="kkk">

                                    <div class="inc-btn">
                                        <button onclick="changeQuantity('{{this._id}}',1)" class="increase items-count"
                                            type="button"><i class="fa fa-plus"></i></button>
                                    </div>
                                    {{!-- id should be unique for changing value or incrementing one will change other.
                                    Id can be received
                                    as cart id from below. --}}
                                    <input class="pl-4 font-weight-bold" style="pointer-events: none;" type="text"
                                        id="{{this._id}}" value="{{this.quantity}}">
                                    <div class="dec-btn">
                                        <button onclick="changeQuantity('{{this._id}}',-1)" class="reduced items-count"
                                            type="button"><i class="fa fa-minus"></i></button>
                                    </div>

                                </div>

                                <div class="d-flex">
                                    <div>
                                        <a href="#" onclick="removeProduct('{{this._id}}')"
                                            class="card-text text-secondary pr-2" style="font-size: 13px;">Remove</a>
                                    </div>
                                    {{!-- <div>
                                        <a href="" class="card-text text-secondary" style="font-size: 13px;">Move to
                                            Wishlist</a>
                                    </div> --}}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                {{/each}}

            </div>
            <div class="col-lg-6">
                {{!-- <div class="row"> --}}
                    
                    {{!-- <div class="col-lg-4 offset-lg-2"> --}}
                        <div class="cart__total__procced">
                            <div id="quantity" class="p-0 m-0">
                                <p class="text-black font-weight-bold">Products</p>
                                {{#each cartData.products}}
                                <div class="d-flex">
                                    <div class="pr-2">
                                        <p class="text-black font-weight-bold m-0">{{inc @index}}.</p>
                                    </div>
                                    <div class="w-75">
                                        <p style="color: #111;" class="text-black font-weight-bold m-0">
                                            {{this.product.productName}}</p>
                                    </div>
                                    <div style="width:20%" class="">
                                        <p class="text-black font-weight-bold m-0"> x{{this.quantity}}</p>
                                    </div>
                                    <div style="width: 15%" class="">
                                        <p style="color: #111;" class="text-danger font-weight-bold m-0">₹
                                            {{this.totalPrice}}</p>
                                    </div>
                                </div>
                                {{/each}}
                            </div>
                            <hr>
                            <h6>Cart total</h6>
                            <ul>

                                {{!-- <li>Subtotal <span>₹ {{price}}</span></li> --}}
                                {{!-- <li>Coupon Discount<span class="text-success">₹ 0</span></li> --}}
                                <li>Total <span id="price">₹ {{price}}</span></li>
                                {{!-- <li>Convenience Fee<span>$ 750.0</span></li> --}}

                            </ul>
                            <a href="/checkout" class="primary-btn">Proceed to checkout</a>
                        </div>
                        {{!--
                    </div> --}}
                    {{!-- </div> --}}
            </div>
        </div>

        <div class="row">
            <div class="col-lg-6 col-md-6 col-sm-12">
                <div class="cart__btn">
                    <a href="#">Continue Shopping</a>
                </div>
            </div>
            {{!-- <div class="col-lg-6 col-md-6 col-sm-6">
                <div class="cart__btn update__btn">
                    <a href="#"><span class="icon_loading"></span> Update cart</a>
                </div>
            </div> --}}
        </div>

    </div>
</section>

